  Hostname console.local
  User alex
  ForwardX11 yes
  ForwardX11Trusted yes

Host console-syncthing
  Hostname console.local
  LocalForward 9999 localhost:8384
  User alex

Host nas
  Hostname nas.local
  User alex

Host farm
  Hostname farm.local
  User root

Host farm-*
  User alex

Host seedbox
  Hostname seedbox.local
  User alex

Host *
  ForwardAgent yes
  StrictHostKeyChecking no
